<h1>New nokia_workout</h1>
<% form_for @nokia_workout, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <%= hidden_field :nokia_workout, :user_id, :value => current_user.id %>
  <p>
    <%= f.label :file %><br />
    <%= f.file_field :file %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
<%= link_to 'Back', nokia_workouts_path %>

