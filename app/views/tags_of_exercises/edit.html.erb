<h1>Editing tags_of_exercise</h1>

<% form_for(@tags_of_exercise) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :exercise %><br />
    <%= f.text_field :exercise %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @tags_of_exercise %> |
<%= link_to 'Back', tags_of_exercises_path %>